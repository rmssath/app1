{
    "family": "taskapp1",
    "networkMode": "awsvpc",
    "containerDefinitions": [
		{
			"name": "App1",
			"image": "<IMAGE1_NAME>",
			"portMappings": [
				{
					"containerPort": 80,
					"hostPort": 80,
					"protocol": "tcp"
				}
			],
			"essential": true
		}
	],
	"requiresCompatibilities": [
		"FARGATE"
	],
	"cpu": "256",
	"memory": "512",
	"executionRoleArn": "arn:aws:iam::aws_account_id:role/ecsTaskExecutionRole"
}
